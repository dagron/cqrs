{
    "name": "gpslab/cqrs",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/gpslab/cqrs",
    "description": "Infrastructure for creating CQRS applications",
    "keywords": [
        "infrastructure",
        "cqrs",
        "psr-3",
        "psr-11",
        "predis",
        "symfony"
    ],
    "autoload": {
        "psr-4": {
            "GpsLab\\Component\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GpsLab\\Component\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "psr/container": "~1.0",
        "psr/log": "~1.0",
        "symfony/dependency-injection": "~2.3|~3.0",
        "symfony/serializer": "~2.3|~3.0",
        "predis/predis": "~1.0|~1.1",
        "superbalist/php-pubsub-redis": "~2.0",
        "phpunit/phpunit": "~4.8",
        "scrutinizer/ocular": "~1.3",
        "satooshi/php-coveralls": "^1.0"
    }
}
